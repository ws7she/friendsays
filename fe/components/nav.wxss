/* pages/components/head.wxss */
.nav-top {
  position: relative;
  background-color:#FEE801;
}
.nav{
  display: flex;
  position: fixed;
  width: 100%;
  color: #000;
  z-index: 99;
  height: 46px;
  
}
.nav-left {
  display: flex;
  align-items:center;
  justify-content:center;
  text-align: center;
  position: absolute;
  left: 10px;
  top: 10px;
}
.nav-title {
  flex: 1;
  text-align: center;
  line-height: 46px;
  font-size: 16px;
  font-weight: bold;
}
.nav_back {
  height: 20px;
  width: 20px;
  transform: rotate(180deg);
  margin-left: 10px;
  margin-right: 5px;
  margin-top: 5px;
}
.nav_home {
  height: 32px;
  width: 32px;
  margin-right: 10px;
}